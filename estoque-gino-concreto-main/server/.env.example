# Set your Gemini/GenAI API key here (server-side)
GEN_AI_API_KEY=your_gemini_api_key_here

# Optional: port
PORT=3001

# Supabase service credentials (server-side). Use service_role key here.
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE=your_service_role_key

# Supabase DB connection string (for automatic migrations). Format: postgresql://user:password@host:5432/postgres
SUPABASE_DB_URL=postgresql://postgres.supabase_project:password@db.supabase.co:5432/postgres
